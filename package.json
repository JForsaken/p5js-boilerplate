{
  "name": "p5js-boilerplate",
  "version": "1.0.0",
  "description": "P5JS starter kit including ES6, Webpack and ESLint.",
  "main": "sketch.js",
  "scripts": {
    "start": "npm install && webpack --display-error-details --progress --colors --watch & npm run serve",
