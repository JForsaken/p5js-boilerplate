    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-json": "^1.2.0",
    "pre-commit": "^1.2.2",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "express": "^4.13.3",
    "lodash": "^4.0.0",
